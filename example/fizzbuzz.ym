def fizzbuzz(num I32)
  if num % 15 == 0
    puts("FizzBuzz")
  else if num % 3 == 0
    puts("Fizz")
  else if num % 5 == 0
    puts("Buzz")
  else
    printf("%d\n".c_str.unsafe_ptr, num)
  end
end

def main() I32
  let num I32 = 0
  while num < 100
    num = num + 1
    fizzbuzz(num)
  end
  return 0
end
