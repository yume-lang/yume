struct Test
  body U8[]
  size I32
end

def capacity(d Test) I32 = d::body.size
def size(d Test) I32 = d::size

def foo(a I32, b I32, c I32) I32 = a

def foo(a I32, b I32) I32 = b

def foo(a U8, b I32) U8 = a

def foo(a I32, b U8) U8 = b

def main() I32
  foo(1, 2, 3)
  foo(4, 5)
  foo(?a, 4)
  foo(5, ?b)
  let d = Test(U8[](0), 0)
  return d.capacity
end
